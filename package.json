{
  "name": "pathfinder",
  "siteName": "PINT",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "NODE_ENV=development npm run build && node scripts/dev.js",
    "dev:prod": "NODE_ENV=production npm run build && http-server ./dist -p $npm_package_config_devPort",
    "reset": "rm -rf node_modules",
    "reset:install": "npm run reset && npm install",
    "update:check": "ncu",
    "update:fix": "ncu -u"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "config": {
    "devPort": 3000
  },
  "dependencies": {
    "browser-sync": "^2.26.7",
    "canvas": "^2.6.0",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "cli-cursor": "^3.1.0",
    "cli-spinners": "^2.2.0",
    "fs-extra": "^8.1.0",
    "html-minifier": "^4.0.0",
    "http-server": "^0.11.1",
    "jsdom": "^15.2.1",
    "lodash.clonedeep": "^4.5.0",
    "npm-check-updates": "^3.2.1",
    "rimraf": "^3.0.0",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {}
}
