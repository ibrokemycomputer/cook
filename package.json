{
  "name": "pathfinder",
  "siteName": "PINT",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "NODE_ENV=development npm run build && node scripts/dev.js",
    "dev:prod": "NODE_ENV=production npm run build && http-server ./dist -p $npm_package_config_devPort",
    "reset": "rm -rf node_modules",
    "reset:install": "npm run reset && npm install",
    "update:check": "ncu",
    "update:fix": "ncu -u"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "config": {
    "devPort": 3000
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "browser-sync": "^2.26.7",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "cli-cursor": "^3.1.0",
    "cli-spinners": "^2.2.0",
    "fs-extra": "^8.1.0",
    "html-minifier": "^4.0.0",
    "http-server": "^0.11.1",
    "imagemin": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.1.0",
    "jsdom": "^15.1.1",
    "lodash.clonedeep": "^4.5.0",
    "npm-check-updates": "^3.1.20",
    "rimraf": "^2.6.3",
    "svgo": "^1.3.0",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {}
}
