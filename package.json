  },
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "clean-css": "^4.2.1",
    "fs-extra": "^7.0.1",
    "html-minifier": "^3.5.21",
    "http-server": "^0.11.1",
    "rimraf": "^2.6.2",
    "uglify-es": "^3.3.9"
  }
}
